<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allstarxi.app" >

    <uses-permission
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.allstarxi.app.activity.NewStartActivity"
            android:label="@string/title_activity_new_start" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name="com.allstarxi.app.activity.StartScreenActivity"
            android:label="Sign Up" >

        </activity>
        <activity
            android:name="com.allstarxi.app.activity.ProfileSetupActivity"
            android:label="@string/title_activity_profile_setup" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.FavoriteTeamActivity"
            android:label="@string/title_activity_favorite_team" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.EmailSignUpActivity"
            android:label="@string/title_activity_email_sign_up" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.AutoDraftStep1Activity"
            android:label="@string/title_activity_auto_draft_step1" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.AutoDraftStep2Activity"
            android:label="@string/title_activity_auto_draft_step2" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.AutoDraftStep3Activity"
            android:label="@string/title_activity_auto_draft_step3" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.TeamPageActivity"
            android:label="@string/title_activity_team_page" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.TransfersActivity"
            android:label="@string/title_activity_transfers" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.LeaguesActivity"
            android:label="@string/title_activity_leagues" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.FixturesActivity"
            android:label="@string/title_activity_fixtures" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.AutoDraftStep4Activity"
            android:label="@string/title_activity_auto_draft_step4" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.TeamPageWithDrawerActivity"
            android:label="@string/title_activity_team_page_with_drawer" >
        </activity>
        <activity
            android:name="com.allstarxi.app.activity.MatchesActivity"
            android:label="@string/title_activity_matches"
            android:parentActivityName="com.allstarxi.app.activity.StartScreenActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.allstarxi.app.StartScreenActivity" />
        </activity>
    </application>

</manifest>
